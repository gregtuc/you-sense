{
  "name": "extension",
  "version": "1.0",
  "description": "open-source extension to keep track of dislikes on youtube videos",
  "permissions": [
    "http://localhost/*",
    "activeTab",
    "webNavigation",
    "http://localhost:3000",
    "http://localhost:3000/",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "manifest_version": 2
}
